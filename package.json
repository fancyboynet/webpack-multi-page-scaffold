{
  "name": "webpack-multi-page-scaffold",
  "version": "0.4.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build-cdn": "cross-env NODE_ENV=production CDN_ENV=true webpack --config ./config/webpack.prod.js",
    "build-no-hash": "cross-env NODE_ENV=production NO_HASH_ENV=true webpack --config ./config/webpack.prod.js",
    "build": "cross-env NODE_ENV=production webpack --config ./config/webpack.prod.js",
    "start": "webpack-dev-server --progress --config ./config/webpack.dev.js",
    "newpage": "node ./script/newpage.js",
    "preinstall": "node ./script/check-install-with-yarn.js",
    "lint": "eslint --fix src/**/*.js"
  },
  "keywords": [
    "webpack",
    "multiple",
    "pages"
  ],
  "author": "Fancy <fancyboynet@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.42",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.46",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0-beta.42",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.46",
    "@babel/preset-env": "^7.0.0-beta.42",
    "autoprefixer": "^9.5.1",
    "babel-eslint": "^8.2.5",
    "babel-loader": "^8.0.0-beta.2",
    "babel-polyfill": "^6.26.0",
    "chalk": "^2.4.1",
    "clean-webpack-plugin": "^1.0.0",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^5.1.4",
    "css-loader": "^2.1.1",
    "eslint": "^5.3.0",
    "eslint-config-standard": "^12.0.0-alpha.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "file-loader": "^1.1.11",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.2.0",
    "install": "^0.11.0",
    "ip": "^1.1.5",
    "less-loader": "^4.1.0",
    "lint-staged": "^8.1.5",
    "mini-css-extract-plugin": "^0.2.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^2.1.5",
    "sass-loader": "^7.1.0",
    "shelljs": "^0.8.2",
    "style-loader": "^0.23.1",
    "svg-inline-loader": "^0.8.0",
    "terser-webpack-plugin": "^1.2.1",
    "url-loader": "^1.0.1",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.2.0",
    "webpack-merge": "^4.1.4"
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0-beta.46",
    "axios": "^0.18.0",
    "lodash": "^4.17.5"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "eslintConfig": {
    "parser": "babel-eslint",
    "parserOptions": {
      "sourceType": "module"
    },
    "extends": [
      "standard"
    ]
  },
  "eslintIgnore": [
    "src/static/**/*.js"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-merge": "cross-env FORCE_COLOR=1 node ./script/check-package.js",
      "post-checkout": "cross-env FORCE_COLOR=1 node ./script/check-package.js"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "eslint --fix",
      "git add"
    ]
  }
}
